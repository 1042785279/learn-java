<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
>
    
    <servlet>
        <servlet-name>ResponseDemo1</servlet-name>
        <servlet-class>com.mio4.web.ResponseDemo1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseDemo1</servlet-name>
        <url-pattern>/servlet/ResponseDemo1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HelloWorldServlet</servlet-name>
        <servlet-class>com.mio4.web.HelloWorldServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorldServlet</servlet-name>
        <url-pattern>/servlet/HelloWorldServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ResponseDemo1_2</servlet-name>
        <servlet-class>com.mio4.web.ResponseDemo1_2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseDemo1_2</servlet-name>
        <url-pattern>/servlet/ResponseDemo1_2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ResponseDemo1_3</servlet-name>
        <servlet-class>com.mio4.web.ResponseDemo1_3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseDemo1_3</servlet-name>
        <url-pattern>/servlet/ResponseDemo1_3</url-pattern>
    </servlet-mapping>

</web-app>